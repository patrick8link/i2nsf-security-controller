<?xml version="1.0" encoding="utf-8"?>
<config xmlns="http://tail-f.com/ns/config/1.0">
  <simple-objects xmlns="http://tail-f.com/ns/simple">
    <number-of-servers>2</number-of-servers>
    <number-of-hosts>2</number-of-hosts>
    <max-number-of-servers>10</max-number-of-servers>
    <max-number-of-hosts>10</max-number-of-hosts>
    <hosts>
      <host>
        <name>saturn@tail-f.com</name>
        <enabled>true</enabled>
        <number-of-servers>2</number-of-servers>
        <servers>
          <server>
            <name>ftp</name>
            <ip>192.168.0.1</ip>
            <port>5001</port>
          </server>
          <server>
            <name>www</name>
            <ip>192.168.0.1</ip>
            <port>5000</port>
          </server>
        </servers>
      </host>
      <host>
        <name>vega@tail-f.com</name>
        <enabled>false</enabled>
        <number-of-servers>0</number-of-servers>
        <servers/>
      </host>
    </hosts>
    <services>
      <service>
        <address>192.168.0.1</address>
        <port-number>5000</port-number>
        <host-name>saturn@tail-f.com</host-name>
        <server-name>www</server-name>
      </service>
      <service>
        <address>192.168.0.1</address>
        <port-number>5001</port-number>
        <host-name>saturn@tail-f.com</host-name>
        <server-name>ftp</server-name>
      </service>
    </services>
  </simple-objects>
</config>
